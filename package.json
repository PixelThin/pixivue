{
  "name": "pixivue",
  "version": "1.0.1",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "echo No build step currently required",
    "build:lib": "yarn build:code && yarn build:copy",
    "build:code": "vue-cli-service build --target lib --name pixivue src/lib.js",
    "build:copy": "copyfiles -f src/components/**/* dist/components",
    "lint": "vue-cli-service lint"
  },
  "main": "src/index.js",
  "browser": {
    "./sfc": "src/components/*"
  },
  "files": [
    "src/**/*"
  ],
  "dependencies": {
    "copyfiles": "^2.2.0",
    "core-js": "^3.6.4"
  },
  "peerDependencies": {
    "axios": "^0.19.0",
    "vue": "^2.6.11",
    "vuetify": "^2.0.0",
    "vue-scroll-reveal": "^1.0.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.0",
    "@vue/cli-plugin-eslint": "^4.3.0",
    "@vue/cli-service": "^4.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  }
}
